@model dynamic
@*<link href="~/Content/custom.css" rel="stylesheet" />*@
<div class="" id="divBlockUI">
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>@Model.PageTitle</h2>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group">
                                <span class="form-inline">
                                    <select id="ddlPager" class="form-control">
                                        <option value="10">10</option>
                                        <option value="20">20</option>
                                        <option value="50">50</option>
                                        <option value="101">All</option>
                                    </select>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 text-right">
                            @if (Model.showAddButton)
                            {
                                <a href="@Model.AddUrl" class=" btn btn-warning"><i class="fa fa-plus-square"> </i> Create</a>
                            }
                        </div>
                    </div>

                    <div class="table-responsive">
                        <table id="tblList" class="table table-striped jambo_table">
                            @{ Html.RenderPartial("HeaderDataTable", (object)Model); }
                            <tbody id="data-list">
                                @{ Html.RenderPartial("DataTable", (object)Model); }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
